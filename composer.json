{
    "name": "anwar/crud-generator",
    "version": "2.0.0",
    "description": "A powerful Laravel package for generating CRUD operations with advanced UI, validation, API generation, and comprehensive testing support.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anwar",
            "email": "ajr.jahid@gmail.com",
            "homepage": "https://github.com/anwar",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/anwar/crud-generator",
    "keywords": [
        "laravel",
        "crud",
        "generator",
        "scaffold",
        "api",
        "validation",
        "bootstrap",
        "admin"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/view": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Anwar\\CrudGenerator\\": "src/"
        },
        "files": [
            "src/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anwar\\CrudGenerator\\AnwarCrudGeneratorProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
